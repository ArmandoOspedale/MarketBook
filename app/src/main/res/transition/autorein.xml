<?xml version="1.0" encoding="utf-8"?>
<transitionSet
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:transitionOrdering="together"
    android:duration="760">
    <slide

        android:slideEdge="top"
        android:interpolator="@android:interpolator/anticipate">
        <targets>
            <target android:excludeId="@android:id/statusBarBackground"/>
            <target android:excludeId="@android:id/navigationBarBackground"/>
            <target android:excludeId="@+id/toolbar2"/>
            <target android:excludeId="@+id/expandablelist"/>
            <target android:excludeId="@+id/mainList"/>
            <target android:excludeId="@+id/autoreLibro"/>
        </targets>
    </slide>
    <fade>
        <targets>
            <target android:targetId="@+id/expandablelist"/>
            <target android:targetId="@+id/mainList"/>
        </targets>
    </fade>
</transitionSet>